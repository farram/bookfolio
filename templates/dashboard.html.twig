				swal({
title: "{{ msg.title|raw }}",
text: "{{ msg.message|raw }}",
icon: "{{ msg.type }}",
buttons: true,
dangerMode: false,
closeOnClickOutside: false,
closeOnEsc: false,
closeModal: true,
buttons: {
confirm: {
text: "Fermer",
value: true,
visible: true,
className: "btn-default text-center center",
closeModal: true
}
}
})
			</script>
